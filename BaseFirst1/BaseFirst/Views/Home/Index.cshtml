@model IEnumerable<BaseFirst.Contact>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>



<input type="submit" value="Add" />
@Html.ActionLink("Editor", "Editor")
<br />
<br/>
<table border="2" width="300" height="150">
    @foreach (BaseFirst.Contact item in this.Model)
    {


        <tr>
            <td> @item.NAme</td>

            <td> @item.LastNAme</td>

            <td> @Html.ActionLink("Dalete", "Dalete", new { id = item.Id })</td>
        </tr>

     }
</table>